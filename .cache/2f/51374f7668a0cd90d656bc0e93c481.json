 100%;\n    position: relative;\n    z-index: 2;\n\n  }\n  .barrel {\n    width: 54.25%;\n    /* left: 7.5%; */\n    position: relative;\n    top: 4.5vw;\n\n  }\n  .spring {\n\twidth: 40.333%;\n    top: 1.5vw;\n    position: relative;\n\n  }\n  .base {\n\twidth:100%;\n\tposition: relative;\n\tz-index: 3;\n\n  }\n  .mag {\n    width: 38%;\n    position: absolute;\n    z-index: 2;\n    right: 1.8vw;\n    -ms-flex-item-align: end;\n    align-self: flex-end;\n    bottom: -3vw;\n\n  }\n  .Section1{\n\t  background: #000000;\n\t  height: 100vh;\n  }\n  .btngrp{\n\tposition: absolute;\n    bottom: 0.5vw;\n    width: 100%;\n    display: flex;\n\tbutton{\n\t\tletter-spacing: 0.15vw;\n\t\tpadding: 0.8vw 1.5vw;\n\t\tmargin: 2vw;\n\t\tmargin-left: 0vw;\n\t\tmargin-right: 0vw;\n\t\tbackground: #1e2022;\n\t\tborder: none;\n\t\tcolor: white;\n\t}\n\tbutton:nth-child(2){\n\t\tpadding: 0.5vw 0.5vw;\n    margin: 2vw;\n\t}\n    justify-content: center;\n\t  h1{\n\t\t  text-transform: uppercase;\n\t  }\n  }\n  #blue{\n\t  background: #3b4d65;\n\t  width: 60px;\n\t  height: 60px;\n\t  border-radius: 50%;\n\t  margin: 2vw;\n  }\n  #black{\n\tmargin: 2vw;\n\twidth: 60px;\n\theight: 60px;\n\tborder-radius: 50%;\n\tmargin-left: 0vw;\n}"},"lineCount":null}},"error":null,"hash":"49cefdf25dfce751b514cc6f45a578eb","cacheData":{"env":{}}}